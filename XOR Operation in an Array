class Solution {
public:
    int xorOperation(int n, int start) {
        int a[10000],i;
        for(i=0;i<n;i++)
            a[i]=start+2*i;
        int xor_arr = 0; 
  
    // Iterating through every element in 
    // the array 
    for (int i = 0; i < n; i++) { 
  
        // Find XOR with the result 
        xor_arr = xor_arr ^ a[i]; 
    } 
  
    // Return the XOR 
    return xor_arr; 
    }
};
